{
	"schemaVersion": 1,
	"id": "oseclient",
	"version": "${version}",
	"name": "OseClient",
	"description": "This is an FabricMC based Minecraft 1.20.X Client",
	"authors": [
		"OseMine"
	],
	"contact": {
		"homepage": "https://fabricmc.net/",
		"sources": "https://github.com/FabricMC/fabric-example-mod"
	},
	"license": "MIT",
	"environment": "client",
	"entrypoints": {
		"main": [
			"de.osemine.oseclient.OseClient"
		],
		"client": ["de.osemine.oseclient.client.OseClientClient"],
		"fabric-datagen": [
			"de.osemine.oseclient.OseClientDataGenerator"
		]
	},
	"mixins": [
		"oseclient.mixins.json"
	],
	"depends": {
		"fabricloader": ">=0.15.6",
		"minecraft": "~1.20.2",
		"java": ">=17",
		"fabric-api": "*"
	},
	"suggests": {
		"another-mod": "*"
	}
}